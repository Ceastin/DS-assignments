#include <iostream>

int main() {
    int array[] = {1, 2, 3, 4, 5}; // Replace this with your array
    int length = sizeof(array) / sizeof(array[0]);
    int sum = 0;

    for (int i = 0; i < length; i++) {
        sum += array[i];
    }

    std::cout << "Sum of the elements in the array: " << sum << std::endl;

    return 0;
}
